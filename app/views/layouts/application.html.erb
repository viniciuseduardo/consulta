<!DOCTYPE html>
<html>
  <head>
    <title>Consulta</title>
    <%= stylesheet_link_tag "application" %>
    <%= csrf_meta_tag %>
  </head>
  <body>
  <ul class="hmenu">
    <%= render 'shared/menu_items' %>
    </ul>
    <%- flash.each do |name, msg| -%>
      <%= content_tag :div, msg, :id => "flash_#{name}" if msg.is_a?(String) %>
    <%- end -%>

    <%= yield %>

  	<%= javascript_include_tag "application" %>
  </body>
</html>
